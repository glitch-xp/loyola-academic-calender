{
  "expo": {
    "name": "Loyola Time Table App",
    "slug": "loyola-timetable-app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#003366"
    },
    "scheme": "loyola",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "url": "https://u.expo.dev/50c8e57e-5092-45df-b5b2-075dd98f4511"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "entryPoint": "expo-router/entry",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loyola.timetableapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "name": "Loyola Time Table",
      "baseUrl": "/loyola-academic-calender",
      "shortName": "Loyola",
      "display": "standalone",
      "backgroundColor": "#003366",
      "themeColor": "#003366",
      "description": "Loyola College Academic Calendar and Time Table App"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/loyola-academic-calender"
    },
    "android": {
      "package": "com.loyola.timetableapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#003366"
      }
    },
    "extra": {
      "eas": {
        "projectId": "50c8e57e-5092-45df-b5b2-075dd98f4511"
      }
    },
    "owner": "glitch_xp"
  }
}